[user]
	email = dallin.christensen@gmail.com
	name = Dallin Christensen
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[diff]
	tool = meld
	guitool = meld
[difftool "meld"]
	cmd = \"C:/Program Files (x86)/Meld/Meld.exe\" $(cygpath -w $LOCAL) $(cygpath -w $REMOTE)
[merge]
	tool = meld
[mergetool "meld"]
	cmd = \"C:/Program Files (x86)/Meld/Meld.exe\" $(cygpath -w $BASE) $(cygpath -w $LOCAL) $(cygpath -w $REMOTE) -o $(cygpath -w $MERGED)
[core]
	editor = nano
